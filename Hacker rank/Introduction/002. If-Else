#!/bin/python

import math
import os
import random
import re
import sys



if __name__ == '__main__':
    n = int(raw_input().strip())
    if n%2 != 0:
        print("Weird")
    elif n%2 == 0:
        for x in range (1, 5, 1):
            if n == x:
                print("Not Weird")
        for x in range (6, 21, 1):
            if n == x:
                print("Weird")
        if  n > 20:
            print("Not Weird")
